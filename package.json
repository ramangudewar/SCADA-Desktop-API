{
  "name": "esd-idas",
  "version": "1.0.2",
  "description": "A Data aquistition application created for modbus Communication by ESD, Pune",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "displayLanguageSelector": false,
      "unicode": true,
      "shortcutName": "ESD-iDAS",
      "warningsAsErrors": true,
      "runAfterFinish": true,
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "menuCategory": false
    },
    "appId": "com.example.myapp",
    "asar": true,
    "win": {
      "icon": "icon.ico"
    }
  },
  "repository": {
    "type": "ESD",
    "url": "git@github.com:serialport/electron-serialport.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "ESD",
  "license": "CC0-1.0",
  "devDependencies": {
    "builder": "^5.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "chart.js": "^4.4.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "exceljs": "^4.4.0",
    "fs": "^0.0.1-security",
    "jspdf": "^2.5.1",
    "path": "^0.12.7",
    "pdfmake": "^0.2.8",
    "serialport": "^11.0.1",
    "tableify": "^1.1.1",
    "xlsx": "^0.18.5",
    "xlsx-style": "^0.8.13"
  }
}
